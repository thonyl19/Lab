@{
	bool isSingleModel = (bool)ViewData["SingleModel"];

	if (isSingleModel)
	{
		Layout = "~/Views/Shared/_LayoutSingle.cshtml";
		@eBundle.QRender_CSS();
		@eBundle.QRender_JS();
	}
}

<div id="app">
    {{A}}
</div>


<script>
    $(() => {
        new Vue({
            el: '#app',
            data() {
                return {
                    A: i18next.t('transferQtyTooMuch').format('A')
                }
            }
        })
    });
</script>
